https://www.analystbuilder.com/questions/company-wide-increase-TytwW?r=J7gqnorKoaz3H7lgpWaz8zXuq
SELECT *,
  round(CASE 
    WHEN pay_level = 1 then salary * 1.1
    WHEN pay_level = 2 then salary * 1.15
    WHEN pay_level = 3 then salary * 3
  end)as new_salary
FROM employees


https://www.analystbuilder.com/questions/bike-price-zKcOR
SELECT round(avg(bike_price),2)
FROM inventory
where bike_price != "NULL" and bike_sold = "Y"
